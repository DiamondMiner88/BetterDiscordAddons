{
  "compilerOptions": {
    "module": "ES6",
    "target": "ES2020",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",

    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,

    "strict": true
  }
}
